generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model AppState {
  id        Int      @id @default(1)
  version   Int      @default(0)
  data      Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Analytics {
  id        Int      @id @default(1)
  data      Json
  updatedAt DateTime @updatedAt
}
